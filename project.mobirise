rap\">\n            <div class=\"col-12 logo-tecfluid\">\n                <img src=\"@PROJECT_PATH@/assets/images/logo-blanco-tecfluid-509x173.png\" alt=\"logo\" title>\n            </div>\n           \n            <div class=\"col-12 col-md-3 mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                <ul class=\"footer-list\">\n                            <li>\n                                <div class=\"has-icon\">\n                                    <img src=\"https://www.tecfluid.cl/mkt/_assets/img/icon-home.png\" alt=\"Home\">\n                                </div>\n                                <div class=\"has-info\">\n                                    <span>\n                                        Av. La Montaña 58. <br>\n                                        Loteo Industral Los Libertadores. <br>\n                                        Colina - Santiago - Chile\n                                    </span>\n                                </div>\n                            </li>\n\n                            <li>\n                                <div class=\"has-icon\">\n                                    <img src=\"https://www.tecfluid.cl/mkt/_assets/img/icon-phone.png\" alt=\"Phone\">\n                                </div>\n                                <div class=\"has-info\">\n                                    <span>\n                                        <a href=\"tel:+56228569500\">\n                                            +56 2 2856 9500\n                                        </a>\n                                    </span>\n                                </div>\n                            </li>\n\n                            <li>\n                                <div class=\"has-icon\">\n                                    <img src=\"https://www.tecfluid.cl/mkt/_assets/img/icon-email.png\" alt=\"Email\">\n                                </div>\n                                <div class=\"has-info\">\n                                    <span>\n                                        <a href=\"mailto:info@tecfluid.cl\">\n                                            info@tecfluid.cl\n                                        </a>\n                                    </span>\n                                </div>\n                            </li>\n                        </ul>\n            </div>\n            \n            <div class=\"col-12 col-md-3 mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                <ul class=\"footer-list\">\n                            <li>\n                                <a href=\"https://www.tecfluid.cl/quienes-somos/\">\n                                    NUESTRA EMPRESA\n                                </a>\n                            </li>\n\n                            <li>\n\n                                <a href=\"https://www.tecfluid.cl/contacto/\">\n                                    CONTÁCTANOS\n                                </a>\n                            </li>\n\n                            <li>\n\n                                <a href=\"https://www.tecfluid.cl/politica-de-privacidad/\">\n                                    POLÍTICA DE PRIVACIDAD\n                                </a>\n                            </li>\n\n                        </ul>\n            </div>\n            \n            <div class=\"col-12 col-md-6 mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                 <img class=\"is100\" src=\"https://www.tecfluid.cl/mkt/_assets/img/iso.png\" alt=\"Iso\">\n            </div>\n        </div>\n        <div class=\"footer-lower\" mbr-if=\"showCopyright\">\n            <div class=\"media-container-row\">\n                <div class=\"col-sm-12\">\n                    <hr>\n                </div>\n            </div>\n            <div class=\"media-container-row mbr-white\">\n                <div class=\"col-sm-6 copyright\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".copyright > p\"></p>\n                </div>\n                <div class=\"col-md-6\">\n                    <div class=\"social-list align-right\" mbr-if=\"iconsCount > 0\">\n                        <div class=\"soc-item\" mbr-if=\"iconsCount>0\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"socicon-twitter socicon mbr-iconfont mbr-iconfont-social\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"iconsCount>1\">\n                            <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                                <span mbr-icon class=\"socicon-facebook socicon mbr-iconfont mbr-iconfont-social\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"iconsCount>2\">\n                            <a href=\"https://www.youtube.com/c/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"socicon-youtube socicon mbr-iconfont mbr-iconfont-social\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"iconsCount>3\">\n                            <a href=\"https://instagram.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"socicon-instagram socicon mbr-iconfont mbr-iconfont-social\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"iconsCount>4\">\n                            <a href=\"https://plus.google.com/u/0/+Mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"socicon-googleplus socicon mbr-iconfont mbr-iconfont-social\"></span>\n                            </a>\n           